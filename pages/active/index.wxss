page{
  height: 100%;
}
.container {
  background: #102a28;
  margin: 0;
  height: 100%;
  height: 1366rpx;
}

.content-bg {
  width: 100%;
  height: 600rpx;
}

.content-bg image {
  width: 100%;
  height: 600rpx;
}

.header {
  position: absolute;
  top: 50rpx;
  text-align: center;
}

.time-top {
  color: #d7b997;
  font-size: 36rpx;
}

.header .time {
  font-size: 30rpx;
  color: #e4f1e9;
}

.btn {
  overflow: hidden;
  width: 80%;
  margin: 0 auto;
  margin-top: 66rpx;
}

.btn button {
  background: none;
  border: none;
  position: relative;
}

.btn button image {
  height: 106rpx;
  line-height: 106rpx;
  width: 376rpx;
}

.btn button text {
  width: 100%;
  position: absolute;
  top: 0;
  color: #3a250f;
  left: 0;
  font-size: 36rpx;
  font-weight: bold;
}

.accelerate, .obtain {
  padding-top: 62rpx;
  text-align: left;
  width: 90%;
  margin: 0 auto;
}
 .title {
  width: 215rpx;
  height: 57rpx;
  background: rgba(224, 195, 163, 1);
  border: 4rpx solid rgba(169, 138, 105, 1);
  font-size: 30rpx;
  font-weight: 800;
  color: rgba(58, 37, 15, 1);
  text-align: center;
  line-height: 57rpx;
  border-top-right-radius: 30rpx;
  border-top-left-radius: 30rpx;
  border-bottom: none;
  margin-left: 15rpx;
}
.content{
  background: #fff;
  border:4rpx solid rgba(169,138,105,1);
  color: #333;
  font-size: 28rpx;
  padding: 30rpx 30rpx 30rpx 30rpx;
  line-height:40rpx;
  font-weight:500;
  border-radius:20rpx;
}

.content-title{
  font-size:30rpx;
font-weight:800;
color:rgba(169,138,105,1);
line-height:48rpx;
border-bottom:1rpx solid rgba(169,138,105,1);
overflow: hidden;
margin-bottom: 10rpx;
padding-top: 20rpx;
}

 .lf{
  float: left;
}
.rf{
  float: right;
height:40rpx;
background:rgba(180,160,131,1);
border-radius:4rpx;
font-size:24rpx;
font-weight:bold;
color:rgba(255,255,255,1);
line-height:40rpx;
text-align: center;

}
.rf:hover{
  background: rgba(180,160,131,1);
}
.myaccelerator{
text-align: center;
color: #D7B997;
font-size: 36rpx;
margin-top: 60rpx;
}
.myaccelerator text{
  color: #fff;
}