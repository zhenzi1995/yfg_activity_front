/*注意设置顶层元素的宽高*/

page, view {
  width: 100%;
  height: 100%;
}

.z-game {
  position: relative;
  overflow: hidden;
}

/*canvas占屏幕一半高度*/

.canvas {
  width: 100%;
  height: 1800rpx;
  margin: 0;
}

swiper {
  height: 100%;
}

.canvas image {
  height: 100%;
  width: 100%;
  max-height: 100%;
  min-height: 100%;
  margin-top: -15rpx;
}

/*写死button的上边距，反正就一个button*/

.list {
  position: absolute;
  top: 140rpx;
  left: 0px;
  height: auto;
  font-size: 24rpx;
  color: #fff;
}

.list .info {
  width: 26%;
  margin-bottom: 4rx;
  background: #fff;
  border: 4rpx solid #91f2fa;
  color: #3fb1bb;
  overflow: hidden;
  font-size: 24rpx;
  border-left: none;
  border-top-right-radius: 30px;
  border-bottom-right-radius: 30px;
  height:60rpx;
  line-height:60rpx;
  display: flex;
  align-items: center
}
.flex1{
  flex: 1;
}
.list .info .ranking {
  width: 46rpx;
  height: 46rpx;
  background: rgba(63, 177, 187, 1);
  border-radius: 50%;
  color: #fff;
  display: inline-block;
  line-height: 46rpx;
  text-align: center;
  margin: 0 3px;
}
.list .info .ct
{
  display: flex;
  align-items: center;
  flex-direction: column;
}
.ct view{
  height: 28rpx;
  line-height:28rpx;
  display: block;
}
.ct .__t{
  overflow: hidden;
  text-overflow: ellipsis;
}

.list view.timer {
  font-size: 36rpx;
  padding-left: 40rpx;
  margin-top: 10rpx;
}

.onlookers {
  position: absolute;
  top: 40rpx;
  text-align: center;
  font-size: 24rpx;
  color: #fff;
}

.onlookers text {
  color: #ffd052;
  font-size: 36rpx;
}

.countdown {
  position: absolute;
  top: 90rpx;
  text-align: center;
  font-size: 60rpx;
  color: #fff;
  font-weight: bold;
}

.countdown view {
  font-weight: 400;
  font-size: 30rpx;
  margin-top: 8rpx;
}

.shiptip {
  text-align: center;
}

.shipname {
  width: 220rpx;
  height: 54rpx;
  background: rgba(255, 255, 255, 1);
  border: 4rpx solid rgba(145, 242, 250, 1);
  border-radius: 46rpx;
  line-height: 54rpx;
  vertical-align: middle;
  text-align: left;
}

.shipname image {
  width: 46rpx;
  height: 46rpx;
  border-radius: 50%;
  margin-right: 9rpx;
  margin-top: 6rpx;
}

.shipname text {
  height: 57rpx;
  line-height: 57rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: rgba(63, 177, 187, 1);
  display: inline-block;
  vertical-align: top;
}
.shiplist{
position: absolute;
display: inline-block;
  left: 38%;
  height: auto;
  width: auto;
}
.ship {
  
  width: 119rpx;
  height: 443rpx;
  
}

.stratbtn image {
  width: 100%;
  height: 54px;
  position: relative;
}

.stratbtn image button {
  position: absolute;
  top: 0;
  background: none;
  border: none;
  outline: none;
  width: 100%;
}

.stratbtn image button:after {
  border: none;
}

.stratbtn image button:active {
  background: none;
  border: none;
}

.stratbtn image button:visited {
  background: none;
  border: none;
}

.stratbtn image button:link {
  background: none;
  border: none;
}

.stratbtn image button:hover {
  background: none;
  border: none;
}

.flt image {
  position: relative;
  height: 108rpx;
  width: 100%;
}

.stratbtn {
  width: 60%;
  float: left;
  height: auto;
  margin-left: 10%;
}

.flt {
  float: left;
  width: 30%;
  text-align: center;
  height: auto;
}

.text-center {
  text-align: center;
  width: 60%;
  margin: 0 auto;
  height: auto;
}

.btns {
  position: absolute;
  bottom: 40rpx;
  height: 200rpx;
}

.acceleratorbtn {
  text-align: center;
  padding-top: 30rpx;
  clear: both;
}

.acceleratorbtn image {
  width: 229rpx;
  height: 53rpx;
  position: relative;
}

.accelerator, .fltbtn {
  position: absolute;
  top: 4rpx;
  background: none;
  border: none;
  color: #fff;
  text-align: center;
  width: 100%;
  padding: 0;
}

.accelerator:after, .fltbtn:after {
  border: none;
}

.accelerator {
  font-size: 30rpx;
  padding-left: 40rpx;
  height: 60rpx;
  line-height: 60rpx;
  top: -4rpx;
}

.storm {
  position: absolute;
  top: 35%;
  left: 24%;
  height: auto;
  display: none;
}

.show {
  display: block;
}

.hide {
  display: none;
}

.storm image {
  width: 50%;
  height: 320rpx;
}

.storm view {
  font-size: 36rpx;
  color: #fff;
  width: 100%;
}

.storm view text {
  color: #ffcf4f;
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 495rpx;
  height: 394rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 9999;
  margin: -247rpx 0 0 -280rpx;
}

.modal-info {
  font-size: 24rpx;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  margin-top: 36rpx;
}
.modal-reward{
font-size:30rpx;
color: #333;
font-weight:bold;
margin-top: 10rpx;
}
.modal-new{
font-size:36rpx;
font-weight:bold;
color:rgba(175,52,14,1);
padding: 12rpx 0 8rpx 0;
}
.modal-look{
font-size:24rpx;
font-weight:bold;
color:rgba(102,102,102,1);
}
.modal-my, .modal-money {
  font-size: 30rpx;
  font-weight: bold;
}
.team-moner .modal-money{
  font-size: 36rpx;
}
.team-moner{
  font-weight: bold;
}
.modal-my {
  color: rgba(58, 37, 15, 1);
  height: auto;
  font-weight: bold;
  padding: 32rpx 0;
}
.modal-t{
  font-size: 36rpx;
  font-weight: bold;
}
.modal-t .modal-money:first-child{
  font-size: 48rpx;
}
.modal-content view{
  height: auto;
  text-align: center;
}
.modal-money {
  color: #927557;
}

.modal-content {
  position: absolute;
  top: 136rpx;
  left: 50rpx;
  text-align: left;
  margin: 0 auto;
}
.team-moner{
  font-size: 30rpx;
}
.modal-ranking {
  font-size: 36rpx;
}

.modal-footer {
  overflow: hidden;
  position: absolute;
  bottom: -100rpx;
  width: 100%;
  text-align: center;
  left: 50rpx;
  height: auto;
}
.modal-bg{
  width: 560rpx;
  height: 531rpx;
}
.modal-footer image {
  width: 280rpx;
  height: 108rpx;
}

.modal-footer text {
  position: absolute;
  width: 100%;
  display: inline-block;
  text-align: center;
  color: #3a250f;
  font-size: 36rpx;
  left: 0;
  height: 106rpx;
  line-height: 106rpx;
  font-weight: bold;
}

.btn-cancel {
  width: 50%;
  color: #666;
  text-align: center;
  border-right: 2rpx solid #dedede;
}
