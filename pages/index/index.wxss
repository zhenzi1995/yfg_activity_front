.container {
  background: #102a28;
  margin: 0;
  position: relative;
}

.content-bg {
  width: 100%;
  height: 600rpx;
}

.content-bg image {
  width: 100%;
  height: 600rpx;
}

.side, .side-bg {
  right: 0;
}

.side-bg {
  position:fixed;
  width: 189rpx;
  top: 48%;
}

.side {
  position: absolute;
  width: 100%;
  top: 25%;
}

.side-bg image {
  width: 179rpx;
  height: 400rpx;
  position: relative;
  right: 0;
}
.side button {
  margin: 0 10rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-bottom: 15rpx;
  background: none;
  border: none;
  font-size: 30rpx;
  border-radius: 0;
  padding: 0;
  position: relative;
  color: #fff;
}

.hr {
  height: 2rpx;
  background: #fff;
  width: 60%;
  margin: 0 auto;
}

.no_record {
  text-align: center;
  color: #999999;
  font-size: 12px;
}
.no_record image {
  width: 197rpx;
  height: 164rpx;
  margin: 400rpx 0;
}

.side button:last-child {
  border-bottom: none;
}

.side button image {
  width: 40rpx;
  height: 20rpx;
  position: absolute;
  top: 0;
}

.side button:after {
  border: none;
}

.side button:active {
  background: none;
  border: none;
}

.btn {
  overflow: hidden;
  position: absolute;
  bottom: -50rpx;
  width: 100%;
  text-align: center;
}

.btn image {
  width: 376rpx;
  height: 106rpx;
}

.btn text {
  position: absolute;
  width: 100%;
  display: inline-block;
  text-align: center;
  color: #3a250f;
  font-size: 36rpx;
  left: 0;
  height: 106rpx;
  line-height: 106rpx;
  font-weight: bold;
}

.num {
  color: #927557;
}

.header {
  position: absolute;
  top: 22rpx;
  text-align: center;
}

.header .time image {
  width: 127rpx;
  position: relative;
  height: 80rpx;
  margin-top: 34rpx;
}

.header .time image.spot {
  width: 10rpx;
  height: 34rpx;
  top: -8rpx;
  margin: 15rpx;
}

.header .time image text {
  position: absolute;
  top: 0;
  height: 80rpx;
  line-height: 80rpx;
  letter-spacing: 30rpx;
  left: 15rpx;
  color: #3a250f;
  font-size: 60rpx;
}

.header .time {
  font-size: 30rpx;
  color: #e4f1e9;
  margin-bottom: 18rpx;
}

.header .time .timenum {
  color: #e0c3a3;
}

.header .time .scene {
  color: #8cb786;
}

.header .person {
  color: #87b67b;
  font-size: 24rpx;
  text-align: center;
}

.header .person text {
  color: #e0c3a3;
  font-size: 50rpx;
}

.rule {
  width: 90%;
  margin: 0 auto;
  position: relative;
  margin-top: 80rpx;
}

.rule-title {
  text-align: center;
  position: relative;
  top: -35rpx;
}

.rule-title text {
  position: absolute;
  left: 0;
  text-align: center;
  display: inline-block;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 48rpx;
  color: #3a250f;
  font-weight: bold;
}

.rule-content {
  position: absolute;
  top: 0;
}

.rule-title image {
  width: 518rpx;
  height: 80rpx;
}

.rule-info {
  width: 85%;
  margin: 0 auto;
}

.rule-info .rule-num {
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  background: #927557;
  color: #fff;
  display: inline-block;
  border-radius: 50%;
  font-size: 30rpx;
  font-weight: bold;
  float: left;
  margin-right: 26rpx;
}

.rule-info view {
  margin-bottom: 20rpx;
  overflow: hidden;
}

.rule-font {
  float: left;
  width: 85%;
  font-size: 36rpx;
}

.rule-bg {
  width: 100%;
  height: 626rpx;
}

.header-logo {
  height: 52rpx;
  width: 406rpx;
}

.header-title {
  height: 104rpx;
  width: 617rpx;
  padding-top: 24rpx;
}

.header-ship {
  height: 146rpx;
  width: 636rpx;
}

.ranking {
  padding: 10px;
  width: 90%;
  margin: 0 auto;
  padding-top: 100rpx;
}

.table {
  position: relative;
}

.table-content {
  padding-top: 27rpx;
  width: 100%;
  background: #fff;
  border: 4rpx solid #a98a69; 
  border-radius: 20rpx;
}
.table-content .tr:last-child{
  border: none;
}

.table-title {
  text-align: center;
  color: #333333;
  font-size: 28rpx;
  font-weight: 600;
  padding: 15px 0 3px;
}

.table-box{
  border-bottom:1px dashed #dddddd;
  padding:15px 0;
}
.table-box .tr:last-child{
  border: none;
}

.tr {
  display: flex;
  width: 89%;
  justify-content: center;
  height: 80rpx;
  align-items: center;
  border-bottom: dashed 1px #a98a69;
  margin: 0 40rpx;
  overflow: hidden
}

.td {
  width: 40%;
  justify-content: center;
  text-align: center;
  font-size: 28rpx;
}

.td-first{
  text-align: left;
  position: relative;
}
.td-first .td-number{
  position: absolute;
  left: 15rpx;
  font-size: 22rpx;
  top: 6rpx;
  color: #fff;
}
.td-first .td-name{
  display: inline-block;
  vertical-align: middle;
  color: #333333;
  position: absolute;
  top: 7rpx;
  left: 50rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  max-width: 80px;
}
.th {
  width: 40%;
  justify-content: center;
  color: #000;
  display: flex;
  height: 80rpx;
  align-items: center;
  font-size: 30rpx;
}

.td .icon-gold {
  width: 40rpx;
  height: 50rpx;
}

.activity-reward {
  width: 100%;
}

.ranking-title {
  overflow: hidden;
  text-align: center;
}

.ranking-title .one-ranking, .ranking-title .history {
  height: 82rpx;
  line-height: 82rpx;
  border: 2px solid #927557;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  color: #3a250f;
  text-align: center;
  font-size: 36rpx;
  float: left;
  border-bottom: none;
  background: #d7b997;
}

.ranking-title .one-ranking {
  width: 400rpx;
  margin-left: 30rpx;
}

.ranking-title .history {
  width: 200rpx;
  margin-left: 2rpx;
}
.modal-footer-btn button{
  position:absolute;
width:100%;
display:inline-block;
text-align:center;
color:#3a250f;
font-size:36rpx;
left:0;
height:106rpx;
line-height:106rpx;
font-weight:bold;
background: none;

}
.active {
  background: #fff !important;
}

.show {
  display: block;
}

.hide {
  display: none;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 495rpx;
  height: 394rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 9999;
  margin: -247rpx 0 0 -240rpx;
}
.modal-showModal{
  width: 100%;
  height: 100%;
}
.modal-footer-btn{
  left: 10rpx !important;
  bottom: -60rpx !important;
}
.showModal-content{
left: 80rpx !important;
width:60%;
margin: 0 auto;
top: 110rpx !important;
}
.showModal-title{
left: 4rpx !important;
}
.modal-showModal-btn{
width: 360rpx;
height: 90rpx;
}
.modal-title {
  padding-top: 50rpx;
  font-size: 36rpx;
  color: #030303;
  text-align: center;
  position: absolute;
  top: -40rpx;
  width: 100%;
  left: 40rpx;
  font-size: r36px;
  font-weight: bold;
  color: rgba(58, 37, 15, 1);
  height: 60rpx;
  line-height: 60rpx;
}

.modal-info {
  font-size: 24rpx;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  margin-top: 36rpx;
}

.modal-my, .modal-money {
  font-size: 30rpx;
  font-weight: bold;
}

.modal-my {
  color: rgba(58, 37, 15, 1);
}

.modal-money {
  color: #927557;
}

.modal-content {
  position: absolute;
  top: 136rpx;
  left: 140rpx;
  text-align: left;
  margin: 0 auto;
  font-size: 12px;
  color: #888;
}

.modal-content .brg {
  position: relative;
  margin-top: 10px;
}

.modal-content .brg:first-child {
  margin: 0;
}

.modal-content .brg::after {
  content: '';
  position: absolute;
  padding: 20rpx;
  border-radius: 100%;
  display: inline-block;
  margin-right: 8px;
  left: -30px;
  top: -3px;
}

.modal-content .brg.green {
  color: #87b67b;
}

.modal-content .brg.green::after {
  background: #87b67b;
}

.modal-content .brg.gray::after {
  background: #999;
}

.modal-footer {
  overflow: hidden;
  position: absolute;
  bottom: -100rpx;
  width: 100%;
  text-align: center;
  left: 50rpx;
}

.modal-footer image {
  width: 376rpx;
  height: 106rpx;
}

.modal-footer text {
  position: absolute;
  width: 100%;
  display: inline-block;
  text-align: center;
  color: #3a250f;
  font-size: 36rpx;
  left: 0;
  height: 106rpx;
  line-height: 106rpx;
  font-weight: bold;
}

.btn-cancel {
  width: 50%;
  color: #666;
  text-align: center;
  border-right: 1px solid #dedede;
}

.btn-confirm {
  width: 50%;
  color: #ec5300;
  text-align: center;
}

/* // 登录弹框 */

.dialog-login .modal-bg {
  height: 350rpx;
  width: 450rpx;
}

.dialog-login {
  width: 395rpx;
  height: 294rpx;
  margin: -247rpx 0 0 -220rpx;
}

.dialog-login .modal-footer {
  left: 30rpx;
}

.dialog-login .modal-footer image {
  width: 276rpx;
  height: 76rpx;
}

.dialog-login .modal-footer button {
  position: absolute;
  width: 100%;
  display: inline-block;
  text-align: center;
  color: #3a250f;
  font-size: 28rpx;
  left: 0;
  font-weight: bold;
  height: 76rpx;
  line-height: 76rpx;
  background: none;
}

.dialog-login .modal-title {
  left: 30rpx;
  top: -53rpx;
}
