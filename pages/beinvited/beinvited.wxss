page{
  height: 100%;
}
.container {
  background: #102a28;
  margin: 0;
  position: relative;
  min-height: 100vh;
}

.content-bg {
  width: 100%;
  height: 600rpx;
}

.content-bg image {
  width: 100%;
  height: 600rpx;
}
.modal-content .brg{
  position: relative;
  margin-top: 10px;
}
.modal-content .brg:first-child{
  margin: 0;
}
.modal-content .brg::after{
  content: '';
  position: absolute;
  padding: 20rpx;
  border-radius: 100%;
  display: inline-block;
  margin-right: 8px;
  left: -30px;
  top: -3px;
}
.modal-content .brg.green{
  color: #87b67b;
}
.modal-content .brg.green::after{
  background: #87b67b;
}
.modal-content .brg.gray::after{
  background: #999999;
}
.header {
  position: absolute;
  top: 50rpx;
  text-align: center;
  width: 100%;
}

.header .time image {
  width: 127rpx;
  position: relative;
  height: 80rpx;
  margin-top: 34rpx;
}

.header .time image.spot {
  width: 10rpx;
  height: 34rpx;
  top: -8rpx;
  margin: 15rpx;
}

.header .time image text {
  position: absolute;
  top: 0;
  height: 80rpx;
  line-height: 80rpx;
  letter-spacing: 30rpx;
  left: 15rpx;
  color: #3a250f;
  font-size: 60rpx;
}

.header .time {
  font-size: 30rpx;
  color: #e4f1e9;
  margin-bottom: 60rpx;
  width: 100%;
}

.time-top {
  font-size: 48rpx;
  color: #e0c3a3;
}

.head {
  text-align: center;
  margin-bottom: 38rpx;
  width: 100%;
}

.head image {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  border: 4rpx solid #a98a69;
}

.name text, .name input {
  height: 30px;
  line-height: 30px;
  vertical-align: middle;
  display: inline-block;
}

.incite-img {
  width: 90%;
  margin: 0 auto;
  padding-top: 20rpx;
}

.incite-img view {
  float: left;
  margin-right: 12rpx;
}

.incite-img view:last-child {
  margin-right: 0;
}

.incite-img view image {
  width: 117rpx;
  height: 117rpx;
  border: 4rpx solid #a98a69;
  border-radius: 20rpx;
  background: #fff;
  overflow: hidden;
}

.btn {
  overflow: hidden;
  width: 80%;
  margin-top: 20rpx;
}
.name {
  background: rgba(255, 255, 255, 1);
  border: 4rpx solid rgba(169, 138, 105, 1);
  border-radius: 20rpx;
  height: 88rpx;
  line-height: 88rpx;
}

.name text, .name input {
  color: #927557;
  text-align: left;
  font-size: 36rpx;
}
.namelist{
  font-size: 48rpx;
  color: #D7B997;
  width: 100%;
}

.incite-info {
  color: #d7b997;
  font-size: 36rpx;
  width: 100%;
  margin: 0 auto;
  text-align: center;
  margin-top: 42rpx;
  margin-bottom: 50rpx;
}

.incite-info text {
  color: #fff;
}

.btn button {
  background: none;
  border: none;
  position: relative;
}

.btn button image {
  height: 106rpx;
  line-height: 106rpx;
  width: 376rpx;
}

.btn button text {
  width: 100%;
  position: absolute;
  top: 0;
  color: #3A250F;
  left: 0;
  font-size: 36rpx;
  font-weight: bold;
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 495rpx;
  height: 394rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 9999;
  margin: -247rpx 0 0 -280rpx;
}

.modal-title {
  padding-top: 50rpx;
  font-size: 36rpx;
  color: #030303;
  text-align: center;
  position: absolute;
  top: -40rpx;
  width: 100%;
  left: 40rpx;
  font-size: r36px;
  font-weight: bold;
  color: rgba(58, 37, 15, 1);
}
.modal-info{
font-size:24rpx;
font-weight:500;
color:rgba(102,102,102,1);
margin-top: 36rpx;
}
.modal-my,.modal-money{
  font-size:30rpx;
font-weight:bold;

}
.modal-my{
color:rgba(58,37,15,1);
}
.modal-money{
  color: #927557;
}
.modal-content {
  position: absolute;
  top: 136rpx;
  left: 140rpx;
  text-align: left;
  margin: 0 auto;
  font-size: 12px;
  color: #888888
}

.modal-footer {
  overflow: hidden;
  position: absolute;
  bottom: -100rpx;
  width: 100%;
  text-align: center;
  left: 50rpx;
}

.modal-footer image {
  width: 376rpx;
  height: 106rpx;
}

.modal-footer text {
  position: absolute;
  width: 100%;
  display: inline-block;
  text-align: center;
  color: #3a250f;
  font-size: 36rpx;
  left: 0;
  height: 106rpx;
  line-height: 106rpx;
  font-weight: bold;
}

.btn-cancel {
  width: 50%;
  color: #666;
  text-align: center;
  border-right: 1px solid #dedede;
}

.btn-confirm {
  width: 50%;
  color: #ec5300;
  text-align: center;
}
